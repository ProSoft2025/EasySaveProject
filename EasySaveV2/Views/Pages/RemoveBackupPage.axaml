<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="EasySaveV2.RemoveBackupPage">
	<StackPanel Margin="20">		
		<TextBlock Text="Remove Backup" FontSize="24" Margin="0 0 0 20"/>
		<TextBlock Text="Job Name"/>
		<TextBox Name="JobNameTextBox" Margin="0 5 0 0"/>
		
		<Button Grid.Row="2" Content="Remove" Click="OnRemoveBackupClick" Classes="proButton" Margin="0 40 0 0"/>
	</StackPanel>
</UserControl>
